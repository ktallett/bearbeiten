{
  "name": "CPlusPlus",
  "displayName": "C++",
  "fileExtensions": [".cpp", ".cxx", ".cc", ".hpp", ".hxx", ".h"],
  "colors": {
    "keywords": "#0000FF",
    "strings": "#A31515",
    "comments": "#008000",
    "numbers": "#FF8C00",
    "functions": "#00008B",
    "classes": "#800080",
    "operators": "#8B0000",
    "preprocessor": "#808080"
  },
  "darkColors": {
    "keywords": "#569CD6",
    "strings": "#CE9178",
    "comments": "#6A9955",
    "numbers": "#B5CEA8",
    "functions": "#DCDCAA",
    "classes": "#4EC9B0",
    "operators": "#D4D4D4",
    "preprocessor": "#C586C0"
  },
  "styles": {
    "keywords": {
      "bold": true,
      "italic": false
    },
    "strings": {
      "bold": false,
      "italic": false
    },
    "comments": {
      "bold": false,
      "italic": true
    },
    "numbers": {
      "bold": false,
      "italic": false
    },
    "functions": {
      "bold": true,
      "italic": false
    },
    "classes": {
      "bold": true,
      "italic": false
    },
    "operators": {
      "bold": true,
      "italic": false
    },
    "preprocessor": {
      "bold": true,
      "italic": false
    }
  },
  "patterns": {
    "keywords": [
      "\\bint\\b", "\\bfloat\\b", "\\bdouble\\b", "\\bchar\\b", "\\bvoid\\b", "\\bbool\\b",
      "\\bint8_t\\b", "\\bint16_t\\b", "\\bint32_t\\b", "\\bint64_t\\b",
      "\\buint8_t\\b", "\\buint16_t\\b", "\\buint32_t\\b", "\\buint64_t\\b",
      "\\bsize_t\\b", "\\bptrdiff_t\\b", "\\bwchar_t\\b", "\\bchar16_t\\b", "\\bchar32_t\\b",
      "\\bif\\b", "\\belse\\b", "\\bfor\\b", "\\bwhile\\b", "\\bdo\\b", "\\bgoto\\b",
      "\\breturn\\b", "\\bbreak\\b", "\\bcontinue\\b", "\\bswitch\\b", "\\bcase\\b",
      "\\bdefault\\b", "\\bstruct\\b", "\\bunion\\b", "\\benum\\b", "\\btypedef\\b",
      "\\bstatic\\b", "\\bextern\\b", "\\bconst\\b", "\\bvolatile\\b", "\\bregister\\b",
      "\\bmutable\\b", "\\binline\\b", "\\bsizeof\\b", "\\balignof\\b", "\\btypeid\\b",
      "\\blong\\b", "\\bshort\\b", "\\bunsigned\\b", "\\bsigned\\b",
      "\\bclass\\b", "\\bpublic\\b", "\\bprivate\\b", "\\bprotected\\b", "\\bfriend\\b",
      "\\bnamespace\\b", "\\busing\\b", "\\btemplate\\b", "\\btypename\\b",
      "\\bvirtual\\b", "\\boverride\\b", "\\bfinal\\b", "\\babstract\\b",
      "\\bnew\\b", "\\bdelete\\b", "\\bthis\\b", "\\boperator\\b",
      "\\btry\\b", "\\bcatch\\b", "\\bthrow\\b", "\\bnoexcept\\b",
      "\\bauto\\b", "\\bdecltype\\b", "\\bconstexpr\\b", "\\bconsteval\\b", "\\bconstinit\\b",
      "\\bnullptr\\b", "\\bstatic_cast\\b", "\\bdynamic_cast\\b",
      "\\bconst_cast\\b", "\\breinterpret_cast\\b", "\\btrue\\b", "\\bfalse\\b",
      "\\bexplicit\\b", "\\bexport\\b", "\\bthread_local\\b", "\\bstatic_assert\\b",
      "\\brequires\\b", "\\bconcept\\b", "\\bco_await\\b", "\\bco_yield\\b", "\\bco_return\\b"
    ],
    "strings": [
      "\"[^\"\\\\]*(\\\\.[^\"\\\\]*)*\"",
      "'[^'\\\\]*(\\\\.[^'\\\\]*)*'",
      "R\"([^()]*)\\([\\s\\S]*?\\)\\1\"",
      "u8\"[^\"]*\"",
      "u\"[^\"]*\"",
      "U\"[^\"]*\"",
      "L\"[^\"]*\""
    ],
    "comments": [
      "//[^\\n]*"
    ],
    "numbers": [
      "\\b\\d+(\\.\\d+)?([eE][+-]?\\d+)?[fFlL]?\\b",
      "\\b0x[0-9a-fA-F]+[uUlL]*\\b",
      "\\b0b[01]+[uUlL]*\\b",
      "\\b0[0-7]+[uUlL]*\\b",
      "\\b\\d+[uUlL]+\\b"
    ],
    "functions": [
      "\\b(\\w+)\\s*\\("
    ],
    "classes": [
      "\\bclass\\s+(\\w+)",
      "\\bstruct\\s+(\\w+)",
      "\\benum\\s+(\\w+)"
    ],
    "operators": [
      "[+\\-*/=<>!&|^~%]",
      "\\+\\+|--|&&|\\|\\||==|!=|<=|>=|<<|>>|::|\\+=|-=|\\*=|/=|%=|&=|\\|=|\\^=|<<=|>>=|->|\\.|\\?:|<=>|\\.\\*|->\\*"
    ],
    "preprocessor": [
      "#\\s*(include|define|undef|ifdef|ifndef|if|elif|else|endif|pragma|error|warning|line)\\b",
      "#[a-zA-Z_]\\w*"
    ]
  },
  "multilineComments": {
    "start": "/*",
    "end": "*/"
  }
}