{
  "name": "Python",
  "displayName": "Python",
  "fileExtensions": [".py", ".pyw", ".pyi"],
  "colors": {
    "keywords": "#0000FF",
    "strings": "#A31515",
    "comments": "#008000",
    "numbers": "#FF8C00",
    "functions": "#00008B",
    "classes": "#800080",
    "operators": "#8B0000",
    "preprocessor": "#808080"
  },
  "styles": {
    "keywords": {
      "bold": true,
      "italic": false
    },
    "strings": {
      "bold": false,
      "italic": false
    },
    "comments": {
      "bold": false,
      "italic": true
    },
    "numbers": {
      "bold": false,
      "italic": false
    },
    "functions": {
      "bold": true,
      "italic": false
    },
    "classes": {
      "bold": true,
      "italic": false
    },
    "operators": {
      "bold": true,
      "italic": false
    },
    "preprocessor": {
      "bold": true,
      "italic": false
    }
  },
  "patterns": {
    "keywords": [
      "\\bclass\\b", "\\bdef\\b", "\\bif\\b", "\\belif\\b", "\\belse\\b",
      "\\bfor\\b", "\\bwhile\\b", "\\btry\\b", "\\bexcept\\b", "\\bfinally\\b",
      "\\bwith\\b", "\\bas\\b", "\\bimport\\b", "\\bfrom\\b", "\\breturn\\b",
      "\\byield\\b", "\\blambda\\b", "\\band\\b", "\\bor\\b", "\\bnot\\b",
      "\\bin\\b", "\\bis\\b", "\\bTrue\\b", "\\bFalse\\b", "\\bNone\\b",
      "\\bpass\\b", "\\bbreak\\b", "\\bcontinue\\b", "\\bglobal\\b", "\\bnonlocal\\b"
    ],
    "strings": [
      "\"[^\"]*\"",
      "'[^']*'",
      "\"\"\"[\\s\\S]*?\"\"\"",
      "'''[\\s\\S]*?'''"
    ],
    "comments": [
      "#[^\\n]*"
    ],
    "numbers": [
      "\\b\\d+(\\.\\d+)?([eE][+-]?\\d+)?\\b"
    ],
    "functions": [
      "\\bdef\\s+(\\w+)",
      "\\b(\\w+)\\s*\\("
    ],
    "classes": [
      "\\bclass\\s+(\\w+)"
    ],
    "operators": [
      "[+\\-*/=<>!&|^~%]",
      "\\b(and|or|not|in|is)\\b"
    ],
    "preprocessor": []
  },
  "multilineComments": {
    "start": "\"\"\"",
    "end": "\"\"\"",
    "alternateStart": "'''",
    "alternateEnd": "'''"
  }
}