{
  "name": "Typst",
  "displayName": "Typst",
  "fileExtensions": [".typ"],
  "colors": {
    "keywords": "#0000FF",
    "strings": "#A31515",
    "comments": "#008000",
    "numbers": "#FF8C00",
    "functions": "#00008B",
    "classes": "#800080",
    "operators": "#8B0000",
    "preprocessor": "#808080",
    "commands": "#0000FF",
    "markup": "#8B008B",
    "variables": "#2E8B57",
    "labels": "#FF6347"
  },
  "styles": {
    "keywords": {
      "bold": true,
      "italic": false
    },
    "strings": {
      "bold": false,
      "italic": false
    },
    "comments": {
      "bold": false,
      "italic": true
    },
    "numbers": {
      "bold": false,
      "italic": false
    },
    "functions": {
      "bold": true,
      "italic": false
    },
    "classes": {
      "bold": true,
      "italic": false
    },
    "operators": {
      "bold": false,
      "italic": false
    },
    "preprocessor": {
      "bold": true,
      "italic": false
    },
    "commands": {
      "bold": true,
      "italic": false
    },
    "markup": {
      "bold": false,
      "italic": true
    },
    "variables": {
      "bold": false,
      "italic": false
    },
    "labels": {
      "bold": false,
      "italic": false
    }
  },
  "patterns": {
    "keywords": [
      "\\blet\\b", "\\bset\\b", "\\bshow\\b", "\\bif\\b", "\\belse\\b", "\\bfor\\b",
      "\\bin\\b", "\\bwhile\\b", "\\bbreak\\b", "\\bcontinue\\b", "\\breturn\\b",
      "\\bimport\\b", "\\binclude\\b", "\\bas\\b", "\\bnot\\b", "\\band\\b", "\\bor\\b",
      "\\bauto\\b", "\\bnone\\b", "\\btrue\\b", "\\bfalse\\b"
    ],
    "functions": [
      "#[a-zA-Z_][a-zA-Z0-9_]*",
      "\\b[a-zA-Z_][a-zA-Z0-9_]*\\s*\\("
    ],
    "commands": [
      "#[a-zA-Z_][a-zA-Z0-9_]*"
    ],
    "markup": [
      "\\*[^*]*\\*",
      "_[^_]*_",
      "`[^`]*`",
      "\\*\\*[^*]*\\*\\*",
      "__[^_]*__",
      "```[\\s\\S]*?```",
      "= [^\\n]*",
      "== [^\\n]*",
      "=== [^\\n]*",
      "==== [^\\n]*",
      "===== [^\\n]*",
      "====== [^\\n]*"
    ],
    "variables": [
      "\\$[a-zA-Z_][a-zA-Z0-9_]*"
    ],
    "labels": [
      "<[a-zA-Z_][a-zA-Z0-9_-]*>",
      "@[a-zA-Z_][a-zA-Z0-9_-]*"
    ],
    "strings": [
      "\"[^\"\\\\]*(\\\\.[^\"\\\\]*)*\"",
      "'[^'\\\\]*(\\\\.[^'\\\\]*)*'"
    ],
    "comments": [
      "//[^\\n]*",
      "/\\*[\\s\\S]*?\\*/"
    ],
    "numbers": [
      "\\b\\d+(\\.\\d+)?(em|pt|pc|in|cm|mm|Q|%)\\b",
      "\\b\\d+(\\.\\d+)?([eE][+-]?\\d+)?\\b",
      "\\b0x[0-9a-fA-F]+\\b"
    ],
    "classes": [],
    "operators": [
      "[+\\-*/=<>!&|^~%]",
      "\\+=|-=|\\*=|/=|==|!=|<=|>=|=>|\\.\\.\\.|\\.\\.=|::|->|<-"
    ],
    "preprocessor": [
      "#import\\b",
      "#include\\b",
      "#set\\b",
      "#show\\b",
      "#let\\b"
    ]
  },
  "multilineComments": {
    "start": "/*",
    "end": "*/"
  }
}