    "tags": {
      "bold": true,
      "italic": false
    },
    "values": {
      "bold": false,
      "italic": false
    }
  },
  "patterns": {
    "keywords": [
      "\\bversion\\b", "\\bencoding\\b", "\\bstandalone\\b", "\\bxmlns\\b",
      "\\bviewBox\\b", "\\bwidth\\b", "\\bheight\\b", "\\bx\\b", "\\by\\b",
      "\\bcx\\b", "\\bcy\\b", "\\br\\b", "\\brx\\b", "\\bry\\b", "\\bd\\b",
      "\\bfill\\b", "\\bstroke\\b", "\\bstroke-width\\b", "\\bstroke-dasharray\\b",
      "\\bopacity\\b", "\\btransform\\b", "\\bclip-path\\b", "\\bmask\\b"
    ],
    "tags": [
      "</?\\b(svg|g|rect|circle|ellipse|line|polyline|polygon|path|text|tspan|defs|use|symbol|marker|clipPath|mask|pattern|linearGradient|radialGradient|stop|animate|animateTransform|animateMotion|set|title|desc|metadata|style|script|foreignObject|image|switch)\\b[^>]*>",
      "<\\?xml[^>]*\\?>",
      "<!DOCTYPE[^>]*>",
      "<!--[\\s\\S]*?-->"
    ],
    "attributes": [
      "\\b(id|class|style|fill|stroke|stroke-width|stroke-dasharray|stroke-linecap|stroke-linejoin|opacity|fill-opacity|stroke-opacity|transform|clip-path|mask|filter|marker-start|marker-mid|marker-end|x|y|width|height|cx|cy|r|rx|ry|x1|y1|x2|y2|points|d|viewBox|preserveAspectRatio|xmlns|version|encoding|standalone)\\b"
    ],
    "strings": [
      "\"[^\"]*\"",
      "'[^']*'"
    ],
    "comments": [
      "<!--[\\s\\S]*?-->"
    ],
    "numbers": [
      "\\b\\d+(\\.\\d+)?([eE][+-]?\\d+)?\\b",
      "\\b\\d+px\\b",
      "\\b\\d+em\\b",
      "\\b\\d+%\\b"
    ],
    "values": [
      "\\b(none|inherit|currentColor|transparent|red|green|blue|black|white|yellow|cyan|magenta|gray|grey|orange|purple|pink|brown|lime|navy|teal|silver|gold|maroon|olive|aqua|fuchsia)\\b",
      "#[0-9a-fA-F]{3,6}\\b",
      "\\brgb\\([^)]*\\)",
      "\\brgba\\([^)]*\\)",
      "\\bhsl\\([^)]*\\)",
      "\\bhsla\\([^)]*\\)",
      "\\burl\\([^)]*\\)"
    ],
    "functions": [],
    "classes": [],
    "operators": [
      "[+\\-*/=<>!&|^~%]"
    ],
    "preprocessor": [
      "<\\?xml[^>]*\\?>",
      "<!DOCTYPE[^>]*>"
    ]
  },
  "multilineComments": {
    "start": "<!--",
    "end": "-->"
  }
}