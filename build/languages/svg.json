    },
    "numbers": {
      "bold": false,
      "italic": false
    },
    "functions": {
      "bold": true,
      "italic": false
    },
    "classes": {
      "bold": true,
      "italic": false
    },
    "operators": {
      "bold": false,
      "italic": false
    },
    "preprocessor": {
      "bold": true,
      "italic": false
    },
    "attributes": {
      "bold": false,
