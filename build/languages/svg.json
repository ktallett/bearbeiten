  "fileExtensions": [".svg"],
  "colors": {
    "keywords": "#0000FF",
    "strings": "#A31515",
    "comments": "#008000",
    "numbers": "#FF8C00",
    "functions": "#00008B",
    "classes": "#800080",
    "operators": "#8B0000",
    "preprocessor": "#808080",
    "attributes": "#FF0000",
    "tags": "#800080",
    "values": "#0000FF"
  },
  "styles": {
    "keywords": {
      "bold": true,
      "italic": false
    },
    "strings": {
