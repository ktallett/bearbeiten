{
  "name": "JavaScript",
  "displayName": "JavaScript",
  "fileExtensions": [".js", ".jsx", ".mjs"],
  "colors": {
    "keywords": "#0000FF",
    "strings": "#A31515",
    "comments": "#008000",
    "numbers": "#FF8C00",
    "functions": "#00008B",
    "classes": "#800080",
    "operators": "#8B0000",
    "preprocessor": "#808080"
  },
  "styles": {
    "keywords": {
      "bold": true,
      "italic": false
    },
    "strings": {
      "bold": false,
      "italic": false
    },
    "comments": {
      "bold": false,
      "italic": true
    },
    "numbers": {
      "bold": false,
      "italic": false
    },
    "functions": {
      "bold": true,
      "italic": false
    },
    "classes": {
      "bold": true,
      "italic": false
    },
    "operators": {
      "bold": true,
      "italic": false
    },
    "preprocessor": {
      "bold": true,
      "italic": false
    }
  },
  "patterns": {
    "keywords": [
      "\\bvar\\b", "\\blet\\b", "\\bconst\\b", "\\bfunction\\b", "\\breturn\\b",
      "\\bif\\b", "\\belse\\b", "\\bfor\\b", "\\bwhile\\b", "\\bdo\\b",
      "\\btry\\b", "\\bcatch\\b", "\\bfinally\\b", "\\bthrow\\b", "\\bnew\\b",
      "\\bthis\\b", "\\btrue\\b", "\\bfalse\\b", "\\bnull\\b", "\\bundefined\\b",
      "\\bclass\\b", "\\bextends\\b", "\\bimport\\b", "\\bexport\\b", "\\bdefault\\b",
      "\\bawait\\b", "\\basync\\b", "\\bof\\b", "\\bin\\b", "\\bswitch\\b", "\\bcase\\b",
      "\\bbreak\\b", "\\bcontinue\\b", "\\btypeof\\b", "\\binstanceof\\b"
    ],
    "strings": [
      "\"[^\"]*\"",
      "'[^']*'",
      "`[^`]*`"
    ],
    "comments": [
      "//[^\\n]*"
    ],
    "numbers": [
      "\\b\\d+(\\.\\d+)?([eE][+-]?\\d+)?\\b",
      "\\b0x[0-9a-fA-F]+\\b",
      "\\b0b[01]+\\b",
      "\\b0o[0-7]+\\b"
    ],
    "functions": [
      "\\bfunction\\s+(\\w+)",
      "\\b(\\w+)\\s*\\(",
      "\\b(\\w+)\\s*=>"
    ],
    "classes": [
      "\\bclass\\s+(\\w+)"
    ],
    "operators": [
      "[+\\-*/=<>!&|^~%]",
      "\\+\\+|--|&&|\\|\\||==|!=|<=|>=|===|!=="
    ],
    "preprocessor": []
  },
  "multilineComments": {
    "start": "/*",
    "end": "*/"
  }
}