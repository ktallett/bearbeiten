{
  "name": "Swift",
  "displayName": "Swift",
  "fileExtensions": [".swift"],
  "colors": {
    "keywords": "#0000FF",
    "strings": "#A31515",
    "comments": "#008000",
    "numbers": "#FF8C00",
    "functions": "#00008B",
    "classes": "#800080",
    "operators": "#8B0000",
    "preprocessor": "#808080"
  },
  "styles": {
    "keywords": {
      "bold": true,
      "italic": false
    },
    "strings": {
      "bold": false,
      "italic": false
    },
    "comments": {
      "bold": false,
      "italic": true
    },
    "numbers": {
      "bold": false,
      "italic": false
    },
    "functions": {
      "bold": true,
      "italic": false
    },
    "classes": {
      "bold": true,
      "italic": false
    },
    "operators": {
      "bold": true,
      "italic": false
    },
    "preprocessor": {
      "bold": true,
      "italic": false
    }
  },
  "patterns": {
    "keywords": [
      "\\bfunc\\b", "\\bvar\\b", "\\blet\\b", "\\bclass\\b", "\\bstruct\\b",
      "\\benum\\b", "\\bif\\b", "\\belse\\b", "\\bfor\\b", "\\bwhile\\b",
      "\\breturn\\b", "\\bimport\\b", "\\bpublic\\b", "\\bprivate\\b",
      "\\binternal\\b", "\\bfileprivate\\b", "\\bopen\\b", "\\bstatic\\b",
      "\\bfinal\\b", "\\boverride\\b", "\\binit\\b", "\\bdeinit\\b",
      "\\bprotocol\\b", "\\bextension\\b", "\\btrue\\b", "\\bfalse\\b",
      "\\bnil\\b", "\\bself\\b", "\\bSelf\\b", "\\bsuper\\b", "\\bin\\b",
      "\\bis\\b", "\\bas\\b", "\\btry\\b", "\\bcatch\\b", "\\bthrow\\b",
      "\\bthrows\\b", "\\brethrows\\b", "\\bguard\\b", "\\bdefer\\b",
      "\\brepeat\\b", "\\bswitch\\b", "\\bcase\\b", "\\bdefault\\b",
      "\\bwhere\\b", "\\bbreak\\b", "\\bcontinue\\b", "\\bfallthrough\\b"
    ],
    "strings": [
      "\"[^\"]*\"",
      "'[^']*'",
      "\"\"\"[\\s\\S]*?\"\"\""
    ],
    "comments": [
      "//[^\\n]*"
    ],
    "numbers": [
      "\\b\\d+(\\.\\d+)?([eE][+-]?\\d+)?\\b",
      "\\b0x[0-9a-fA-F]+\\b",
      "\\b0o[0-7]+\\b",
      "\\b0b[01]+\\b"
    ],
    "functions": [
      "\\bfunc\\s+(\\w+)",
      "\\b(\\w+)\\s*\\("
    ],
    "classes": [
      "\\bclass\\s+(\\w+)",
      "\\bstruct\\s+(\\w+)",
      "\\benum\\s+(\\w+)",
      "\\bprotocol\\s+(\\w+)"
    ],
    "operators": [
      "[+\\-*/=<>!&|^~%]",
      "\\+\\+|--|&&|\\|\\||==|!=|<=|>=|\\?\\?|\\.\\.\\.|\\.\\.<?",
      "\\+=|-=|\\*=|/=|%=|&=|\\|=|\\^=|<<=|>>="
    ],
    "preprocessor": [
      "@\\w+"
    ]
  },
  "multilineComments": {
    "start": "/*",
    "end": "*/"
  }
}